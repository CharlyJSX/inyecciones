<%- include("partials/_header") %>

<div class="container mt-5">
  <div class="row">
    <div class="col-md-5 offset-md-3 card bg-dark">
      <div class="card-body">
        <form action="/inyeccionesUpdate/<%= data.id %>" method="POST">
        
          <input
            type="text"
            name="name"
            value="<%= data.name %>"
            placeholder="Nombre"
            class="form-control mb-3"
            autofocus
          />
          <input
            type="number"
            name="mes"
            value="<%= data.mes%>"
            placeholder="Mes"
            class="form-control mb-3"
          />
          <input
            type="date"
            name="time"
            value="<%= data.time%>"
            placeholder="Fecha"
            class="form-control mb-3"
          />
             <input
            type="text"
            name="phone"
            value="<%= data.phone%>"
            placeholder="Teléfono"
            class="form-control mb-3"
          />
             <input
            type="email"
            name="emal"
            value="<%= data.email%>"
            placeholder="Correo Eléctronico"
            class="form-control mb-3"
          />


          <button type="submit" class="btn btn-primary">Actualizar Usuaria</button>
        </form>
      </div>
    </div>
  </div>
</div>

<%- include("partials/_footer") %>
